{
  "name": "svelte-windicss-preprocess",
  "version": "2.1.3",
  "description": "A Svelte Preprocessor to compile tailwindcss at build time based on windicss compiler.",
  "main": "index.js",
  "types": "index.d.ts",
  "browser": "browser.js",
  "scripts": {
    "dev": "rollup -cw",
    "tsd": "tsc -p ./src --emitDeclarationOnly",
    "test": "ts-node -O '{\"module\":\"commonjs\"}' node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "build": "rollup -c && npm run tsd",
    "product": "PUBLISH=true rollup -c && npm run tsd"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voorjaar/svelte-windicss-preprocess.git"
  },
  "keywords": [
    "svelte",
    "tailwindcss",
    "css",
    "compiler"
  ],
  "author": "Veritas Raven",
  "license": "MIT",
  "homepage": "https://github.com/voorjaar/svelte-windicss-preprocess",
  "dependencies": {
    "magic-string": "^0.25.7",
    "windicss": "^2.1.3"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@rollup/plugin-typescript": "^8.1.1",
    "@types/jasmine": "^3.6.3",
    "@types/node": "^14.14.14",
    "jasmine": "^3.6.4",
    "jasmine-spec-reporter": "^6.0.0",
    "rollup": "^2.38.5",
    "rollup-plugin-terser": "^7.0.2",
    "ts-node": "^9.1.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
}
